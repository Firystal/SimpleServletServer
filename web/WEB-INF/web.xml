<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>Main</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/Main</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SignUpUI</servlet-name>
        <servlet-class>SignUp.SignUpUI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpUI</servlet-name>
        <url-pattern>/SignUpUI</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>SignUp.SignUp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginUI</servlet-name>
        <servlet-class>Login.LoginUI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginUI</servlet-name>
        <url-pattern>/LoginUI</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>UserAct.LogOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Close</servlet-name>
        <servlet-class>UserAct.Close</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Close</servlet-name>
        <url-pattern>/Close</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserInfo</servlet-name>
        <servlet-class>UserAct.UserInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserInfo</servlet-name>
        <url-pattern>/UserInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Events</servlet-name>
        <servlet-class>UserAct.Events</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Events</servlet-name>
        <url-pattern>/Events</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddEvent</servlet-name>
        <servlet-class>UserAct.AddEvent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddEvent</servlet-name>
        <url-pattern>/AddEvent</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteEvent</servlet-name>
        <servlet-class>UserAct.DeleteEvent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteEvent</servlet-name>
        <url-pattern>/DeleteEvent</url-pattern>
    </servlet-mapping>
</web-app>